{
  "manifest_version": 2,

  "name": "Tree History",
  "description": "This extension demonstrates history as tree.",
  "version": "3.0",

  "permissions": [
    "tabs",
    "history"
  ],
  
  "background": {
    "scripts": ["navigationsListener.js"],
    "persistent": true
  },
  
  "chrome_url_overrides" : {
    "history": "main.html"
  }
  
}
